{
  "name": "jlo-my-storybook-component",
  "private": false,
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://jl-olemar.github.io/My-First-Storybook/",
  "repository": {
    "url": "https://github.com/JL-OLEMAR/My-First-Storybook",
    "type": "git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "storybook",
    "chromatic",
    "typescript",
    "vite"
  ],
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "rimraf dist/ && tsc && npm run copy-css-to-dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=29a632a2bbb5",
    "copy-css-to-dist": "copyfiles -u 1 src/**/*.css dist/"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@vitejs/plugin-react": "1.3.0",
    "babel-loader": "8.2.5",
    "browserslist": "4.20.2",
    "chromatic": "6.5.4",
    "copyfiles": "2.4.1",
    "css-loader": "6.7.1",
    "postcss": "8.4.12",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "typescript": "4.6.3",
    "vite": "2.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
}
